securityUtility encode --encoding=aes GiveMeLiberty

securityUtility createSSLCertificate 
--server=myserver 
--password=mypassword 
--validity=365
--subject=CN=mycompany,O=myOrg,C=myCountry
securityUtility help createSSLCertificate

